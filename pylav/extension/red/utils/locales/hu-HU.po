msgid "Show the version of PyLav library"
msgstr "A PyLav-kódtár verziójának megjelenítése"

#: pylav/extension/red/utils/required_methods.py:91
msgid "Library"
msgstr "Könyvtár"

#: pylav/extension/red/utils/required_methods.py:92
msgid "Version"
msgstr "Verzió"

#: pylav/extension/red/utils/required_methods.py:111
#, docstring
msgid "Sync the Bots slash commands"
msgstr "A botok perjeles parancsainak szinkronizálása"

#: pylav/extension/red/utils/required_methods.py:120
msgid "Synced the bots slash commands"
msgstr "Szinkronizálta a robotok perjeles parancsait"

#: pylav/extension/red/utils/required_methods.py:141
msgid "This command requires an existing player to be run"
msgstr "Ehhez a parancshoz egy meglévő lejátszó futtatása szükséges"

#: pylav/extension/red/utils/required_methods.py:150
msgid "PyLavPlayer cog is currently temporarily unavailable due to an outage with the backend services, please try again later"
msgstr "A PyLavPlayer fogaskereke jelenleg ideiglenesen nem érhető el a háttérszolgáltatások leállása miatt, kérjük, próbálja újra később"

#: pylav/extension/red/utils/required_methods.py:154
msgid "No Lavalink node currently available"
msgstr "Jelenleg nem érhető el Lavalink csomópont"

#: pylav/extension/red/utils/required_methods.py:163
msgid "PyLavPlayer is currently unable to process tracks belonging to {feature}"
msgstr "A PyLavPlayer jelenleg nem tudja feldolgozni a {feature}"

#: pylav/extension/red/utils/required_methods.py:166
msgid "No Lavalink node currently available with feature {feature}"
msgstr "Jelenleg egyetlen Lavalink csomópont sem érhető el funkcióval {feature}"

#: pylav/extension/red/utils/required_methods.py:177
msgid "This command is not available in this channel. Please use {channel}"
msgstr "Ez a parancs nem érhető el ebben a csatornában. Kérjük, használja {channel}"

#: pylav/extension/red/utils/required_methods.py:189
msgid "This command requires you to be a DJ"
msgstr "Ehhez a parancshoz DJ-nek kell lenned"

#: pylav/extension/red/utils/required_methods.py:227
msgid "PyLav is not ready - Please try again shortly"
msgstr "A PyLav még nem áll készen - Kérjük, próbálja újra hamarosan"

